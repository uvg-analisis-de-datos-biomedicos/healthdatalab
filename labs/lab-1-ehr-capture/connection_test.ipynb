    "JOIN admissions a\n",
    "    ON le.hadm_id = a.hadm_id\n",
    "JOIN patients p\n",
    "    ON a.subject_id = p.subject_id\n",
    "WHERE dli.label = 'Creatinine'\n",
    "GROUP BY\n",
    "    a.hadm_id,\n",
    "    p.external_id,\n",
    "    p.full_name;\n",
    "\"\"\"\n",
    "df = pd.read_sql(query, engine)\n",
