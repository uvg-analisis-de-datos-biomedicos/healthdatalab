    "query = \"\"\"\n",
    "SELECT\n",
    "    a.hadm_id,\n",
    "    p.external_id,\n",
    "    p.full_name,\n",
    "    MAX(\n",
    "        CASE\n",
    "            WHEN dli.label = 'White Blood Cells'\n",
    "            THEN le.value_num\n",
    "        END\n",
    "    ) AS max_wbc,\n",
    "    MAX(\n",
    "        CASE\n",
    "            WHEN dli.label = 'Lactate'\n",
    "            THEN le.value_num\n",
    "        END\n",
    "    ) AS max_lactate\n",
    "FROM labevents le\n",
    "JOIN d_labitems dli\n",
    "    ON le.labitem_id = dli.labitem_id\n",
    "JOIN admissions a\n",
    "    ON le.hadm_id = a.hadm_id\n",
    "JOIN patients p\n",
    "    ON a.subject_id = p.subject_id\n",
    "WHERE dli.label IN ('White Blood Cells','Lactate')\n",
    "GROUP BY\n",
    "    a.hadm_id,\n",
    "    p.external_id,\n",
    "    p.full_name\n",
    "HAVING\n",
    "    MAX(\n",
    "        CASE\n",
    "            WHEN dli.label = 'White Blood Cells'\n",
    "            THEN le.value_num\n",
    "        END\n",
    "    ) > 12\n",
    "    OR\n",
    "    MAX(\n",
    "        CASE\n",
    "            WHEN dli.label = 'Lactate'\n",
    "            THEN le.value_num\n",
    "        END\n",
    "    ) > 2;\n",
    "\"\"\"\n",
    "df = pd.read_sql(query, engine)\n",
    "df\n"
   ]
