    "query = \"\"\"\n",
    "SELECT\n",
    "  a.hadm_id,\n",
    "  p.external_id,\n",
    "  p.full_name,\n",
    "  EXTRACT(EPOCH FROM (a.dischtime - a.admittime)) / 86400.0\n",
    "    AS length_of_stay_days\n",
    "FROM admissions a\n",
    "JOIN patients p ON p.subject_id = a.subject_id\n",
    "ORDER BY length_of_stay_days DESC;\n",
    "\"\"\"\n",
    "\n",
    "df_los = pd.read_sql(query, engine)\n",
    "df_los\n"
