    "query = \"\"\"\n",
    "SELECT\n",
    "    p.external_id,\n",
    "    p.full_name,\n",
    "    COUNT(*) AS n_admissions\n",
    "FROM patients p\n",
    "JOIN admissions a\n",
    "    ON p.subject_id = a.subject_id\n",
    "GROUP BY\n",
    "    p.subject_id,\n",
    "    p.external_id,\n",
    "    p.full_name\n",
    "HAVING COUNT(*) > 1;\n",
    "\"\"\"\n",
    "df = pd.read_sql(query, engine)\n",
    "df\n"
   ]
